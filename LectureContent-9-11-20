function countSalary(fastLön, rörligLön) {
  
  var slutLön = fastLön + rörligLön
  console.log(slutLön) //

   return slutLön

}

var rakibsLon = countSalary(2, 3);

var sebbe = countSalary(3,4);

console.log(rakibsLon + sebbe)

