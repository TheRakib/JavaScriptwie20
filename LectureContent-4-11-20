// Vi ska skapa en formulär  i html 
// vi ska skapa en input fält 
// läsa data från input fältet 
  // event ??  händelse som händer när använder din webb appen
          // till ex. onSubmit/ submit, onChange/ change, 
           // onLoad /Load, onClick  / Click 
           // i html : onChange : börjar med on
           // AddEventListner : change
  // function ??    en kode block som kan utföra något task/arbete/uppgift
        //function kan lyssna på en event och event kan anropa funktionen 
// Skriver vi ut input data in i div tagg i html 
// 10.30
// https://developer.mozilla.org/en-US/docs/Web/Events
// skapa en function som tar input data och skriver in i div
 function submitData() {   
  // det är en form och när vi submit form uppdaterar sidan 
  // data försvinner
  var inputData = document.querySelector("#text").value;
  //console.log(inputData)
  document.querySelector("div").innerText = inputData;
  // Vi ska skriva inputData in i div tagg i html
//console.log("vad som helst")
 }
 // när användare trycker på button då vill vi submitData functionen ska anropas 
    //  arguement 
           //event, Functionnamn
 //addEventListner("submit" , submitData)
 var button = document.querySelector("button")
  button.addEventListener( "click",  submitData );

  // 11.30 

