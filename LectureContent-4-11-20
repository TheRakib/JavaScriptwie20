app.js : 

// koden börjar 
var h1 =  document.querySelector("h1")
 

 
// vi skulle skapa en function som läser input data och skriver in i 
// en div element. 
function collectData() {
 //läser input fält 
  var inputData = document.querySelector("#text").value;  
// valde div taggen
  var div = document.querySelector("div")
// skriver inputData in i div 
  div.textContent = inputData;
}



const btn=   document.querySelector("button")
btn.addEventListener("click", collectData)
 //document.addEventListener("")
 
 //click, submit , change , load

   console.log(inputData)
